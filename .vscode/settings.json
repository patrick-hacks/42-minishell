{
	"files.associations": {
		"*.h": "c",
		"*.c": "c",
		"__string": "c",
		"__tuple": "c",
		"algorithm": "c",
		"bitset": "c",
		"cmath": "c",
		"functional": "c",
		"iterator": "c",
		"locale": "c",
		"ratio": "c",
		"string": "c",
		"vector": "c"
	},
	"[c]": {
		"editor.defaultFormatter": "keyhr.42-c-format",
	},
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"cpplint.excludes": [
		".."
	],
	"C/C++ Include Guard.Path Depth": 1,
	"C/C++ Include Guard.Path Skip": 10,
	"C/C++ Include Guard.Suffix": "",
}
